{
  "name": "carrentalproject",
  "main": "app/index.tsx",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "detox": {
    "testRunner": "jest",
    "runnerConfig": "e2e/config.json",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "path/to/your/ios/build/Build/Products/Debug-iphonesimulator/YourApp.app",
        "build": "xcodebuild -project ios/YourApp.xcodeproj -scheme YourApp -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 12"
        }
      },
      "android.emu.debug": {
        "binaryPath": "path/to/your/android/build/app/debug/app-debug.apk",
        "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "type": "android.emulator",
        "device": {
          "avdName": "Pixel_3a_API_30_x86"
        }
      }
    }
  },
  "dependencies": {
    "@aws-amplify/react-native": "^1.0.13",
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/netinfo": "11.3.1",
    "@react-native-picker/picker": "^2.7.5",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^2.2.3",
    "@tanstack/react-query": "^5.32.0",
    "amazon-cognito-identity-js": "^6.3.10",
    "aws-amplify": "^5.3.15",
    "expo": "^51.0.5",
    "expo-font": "~12.0.7",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.12",
    "expo-splash-screen": "~0.27.4",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.4",
    "expo-web-browser": "~13.0.3",
    "html-react-parser": "^5.1.10",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.1",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-svg": "^15.3.0",
    "react-native-web": "~0.19.6",
    "react-redux": "^9.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@testing-library/react-native": "^12.4.3",
    "@types/detox": "^18.1.0",
    "@types/expo": "^33.0.1",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.79",
    "@types/react-native": "^0.73.0",
    "@types/react-redux": "^7.1.33",
    "detox": "^20.23.0",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.1",
    "react-native-svg-transformer": "^1.4.0",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.5",
    "typescript": "^5.5.2"
  },
  "private": true
}
